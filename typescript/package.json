{
  "name": "denied-sdk",
  "version": "0.4.0",
  "description": "Denied SDK for TypeScript",
  "keywords": [
    "denied",
    "sdk",
    "typescript",
    "authorization"
  ],
  "homepage": "https://github.com/denied-dev/denied-sdk#readme",
  "bugs": {
    "url": "https://github.com/denied-dev/denied-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denied-dev/denied-sdk.git",
    "directory": "typescript"
  },
  "license": "Apache-2.0",
  "author": "Denied team",
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "axios": "^1.8.0"
  },
  "peerDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/claude-agent-sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.0",
    "vitest": "^2.1.8",
    "zod": "^3.24.1"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 88,
    "tabWidth": 2,
    "arrowParens": "always"
  }
}
